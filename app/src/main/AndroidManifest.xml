<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mike.appgranja">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".addGranja"
            android:label="Añadir Granja"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".addNave"
            android:label="Añadir Nave"
            android:parentActivityName=".VistaNave" />
        <activity
            android:name=".VistaNave"
            android:label="Naves"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Clases.infoNave"
            android:label="Info Nave"
            android:parentActivityName=".MainActivity" ></activity>
    </application>

</manifest>
